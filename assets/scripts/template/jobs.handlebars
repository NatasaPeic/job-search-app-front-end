  <!-- I can't overwrite damn Bootstrap styleshes, so I had to use inline td styling -->

<tr class="table-top-row" style="border: 1px solid black;">
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">POSITION</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">COMPANY NAME</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">COMAPNY URL</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">REQUIREMENTS</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">SALARY</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">APPLICATION URL</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">NOTES</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">APPLIED</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">CONTACT</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">UPDATES</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; ">DELETE</td>
</tr>

<tr class="table-top-row" style="border: 1px solid black;">

     {{#each this}}
     {{#each jobs}}
  <tr>


    <td style="padding:10px;">{{this.company_name}}</td>
    <td style="padding:10px;">{{this.position_name}}</td>
    <td style="padding:10px;">{{this.company_url}}</td>
    <td style="padding:10px;">{{this.requirements}}</td>
    <td style="padding:10px;">{{this.salary}}</td>
    <td style="padding:10px;">{{this.application_url}}</td>
    <td style="padding:10px;">{{this.notes}}</td>
    <td style="padding:10px;">{{this.applied}}</td>
    <td style="padding:10px;">{{this.contact}}</td>
    <td><button type="button" class="btn btn-primary update-job" data-job-id="{{id}}" data-toggle="modal" data-target="#update-job-modal">Update</button></td>
    <td><button type="button" class="btn btn-danger delete-job" data-job-id="{{id}}" data-toggle="modal" data-target="#delete-job-modal">Delete</button></td>
  </tr>
  {{/each}}

  <!-- I wasn't able to overwrite Bootstrap css, so I had to use inline td styling -->

<tr class="table-top-row">
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">POSITION</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">COMPANY NAME</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">COMAPNY URL</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">REQUIREMENTS</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">SALARY</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">APPLICATION URL</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">NOTES</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">APPLIED</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">CONTACT</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">UPDATES</td>
  <td style="padding:10px; width: 150px; background-color: #de7006; font-size: 20px; font-family: inherit; border: 1px solid black;">DELETE</td>
  

</tr>
<tr class="table-top-row">

     {{#each this}}
     {{#each jobs}}
  <tr>


    <td style="padding:10px;">{{this.company_name}}</td>
    <td style="padding:10px;">{{this.position_name}}</td>
    <td style="padding:10px;">{{this.company_url}}</td>
    <td style="padding:10px;">{{this.requirements}}</td>
    <td style="padding:10px;">{{this.salary}}</td>
    <td style="padding:10px;">{{this.application_url}}</td>
    <td style="padding:10px;">{{this.notes}}</td>
    <td style="padding:10px;">{{this.applied}}</td>
    <td style="padding:10px;">{{this.contact}}</td>
    <td><button type="button" class="btn btn-primary update-job" data-job-id="{{id}}" data-toggle="modal" data-target="#update-job-modal">Update</button></td>
    <td><button type="button" class="btn btn-danger delete-job" data-job-id="{{id}}" data-toggle="modal" data-target="#delete-job-modal">Delete</button></td>
  </tr>
  {{/each}}

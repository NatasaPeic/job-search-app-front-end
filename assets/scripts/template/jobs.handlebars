  <!-- I can't overwrite damn Bootstrap styleshes, so I had to use inline td styling -->

<tr class="table-top-row">
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Position</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Company</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Company URL</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Requirements</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Salary</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Application URL</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Notes</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Submitted</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">Contact</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">UPDATE JOB</td>
  <td style="padding:10px; width: 150px; background-color: #6E6E6E; color: white; font-size: 20px; font-family: inherit; ">DELETE JOB</td>
</tr>

<tr class="table-top-row">

     {{#each this}}
     {{#each jobs}}

  <tr>
    <td style="padding:10px;">{{this.company_name}}</td>
    <td style="padding:10px;">{{this.position_name}}</td>
    <td style="padding:10px;">{{this.company_url}}</td>
    <td style="padding:10px;">{{this.requirements}}</td>
    <td style="padding:10px;">{{this.salary}}</td>
    <td style="padding:10px;">{{this.application_url}}</td>
    <td style="padding:10px;">{{this.notes}}</td>
    <td style="padding:10px;">{{this.applied}}</td>
    <td style="padding:10px;">{{this.contact}}</td>
    <td><button type="button" class="btn btn-success update-job" data-job-id="{{id}}" data-toggle="modal" data-target="#update-job-modal">Update</button></td>
    <td><button type="button" class="btn btn-warning delete-job" data-job-id="{{id}}" data-toggle="modal" data-target="#delete-job-modal">Delete</button></td>
  </tr>
  {{/each}}
<!-- {{/each}} -->
